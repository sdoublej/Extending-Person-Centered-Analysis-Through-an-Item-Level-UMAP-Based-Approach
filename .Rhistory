data <- read.csv(reversed_df.csv)
data <- read.csv('reversed_df.csv')
data
data['MAS_MA_items']
X4 <- data[, c("MAS_MA_items", "MAS_DA_r_items", "MAS_DA_f_items", "MAS_EA_items")]
colnames(data)
colSums(is.na(X4))
nrow(X4)
X4 <- na.omit(X4)
nrow(X4)
library(tidyLPA)
set.seed(2026)
res4 <- estimate_profiles(
X4,
n_profiles = 1:6,
models = 1
)
library(dplyr)
keys <- grep("^model_1_class_\\d+$", names(res4), value = TRUE)
fit_table <- do.call(rbind, lapply(keys, function(k) {
fit <- res4[[k]]$fit
K <- as.integer(sub(".*_class_(\\d+)$", "\\1", k))
data.frame(
solution = paste0(K, " class"),
LogLik   = unname(fit[["LogLik"]]),
AIC      = unname(fit[["AIC"]]),
BIC      = unname(fit[["BIC"]]),
sBIC     = unname(fit[["SABIC"]]),
Entropy  = unname(fit[["Entropy"]]),
n_min    = unname(fit[["n_min"]]),
stringsAsFactors = FALSE
)
})) %>%
arrange(as.integer(sub(" class","",solution)))
fit_table_round <- fit_table %>%
mutate(
across(c(LogLik, AIC, BIC, sBIC), ~ round(., 3)),
Entropy = round(Entropy, 3),
n_min   = round(n_min, 3)
)
fit_table_round
View(res4)
View(data)
